{
  "name": "pendragon6thedition",
  "version": "1.0.1",
  "description": "You need to run npm start to get the compilers going. Saves the pendragon.scss file to compile that code.",
  "dependencies": {
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6"
  },
  "scripts": {
    "start": "npm-run-all --parallel compile:sass compile:pug",
    "compile:sass": "node-sass pendragon.scss pendragon.css -w",
    "compile:pug": "pug pendragon.pug pendragon.html -w"
  },
  "keywords": [
    "Roll20"
  ],
  "license": "ISC"
}
